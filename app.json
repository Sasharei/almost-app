{
  "expo": {
    "name": "Almost",
    "slug": "almost_clean",
    "version": "2.0.14",
    "orientation": "default",
    "icon": "./assets/Almost_icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": false,
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "bundleIdentifier": "com.sasarei.almostclean",
      "supportsTablet": true,
      "googleServicesFile": "./ios/GoogleService-Info.plist",
      "infoPlist": {
        "NSCameraUsageDescription": "Almost lets you snap a portrait so your savings profile feels personal.",
        "NSPhotoLibraryUsageDescription": "Almost accesses your photos to update the avatar that keeps you motivated."
      }
    },
    "android": {
      "package": "com.sasarei.almostclean",
      "versionCode": 21,
      "adaptiveIcon": {
        "foregroundImage": "./assets/Almost_icon.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "permissions": [
        "android.permission.CAMERA",
        "android.permission.POST_NOTIFICATIONS"
      ]
    },
    "web": {
      "favicon": "./assets/Almost_icon.png"
    },
    "plugins": [
      [
        "expo-image-picker",
        {
          "photosPermission": "Almost needs access to your photos to personalise your savings profile.",
          "cameraPermission": "Allow Almost to use the camera for quick profile snapshots."
        }
      ],
      "expo-notifications",
      "expo-font",
      [
        "expo-splash-screen",
        {
          "backgroundColor": "#ffffff",
          "image": "./assets/splash-icon.png",
          "resizeMode": "contain"
        }
      ],
      "@react-native-firebase/app"
    ],
    "extra": {
      "eas": {
        "projectId": "f2adb262-e091-47a0-bb3a-09b067fd148a"
      }
    },
    "runtimeVersion": {
      "policy": "appVersion"
    },
    "updates": {
      "enabled": false,
      "checkAutomatically": "ON_ERROR_RECOVERY",
      "fallbackToCacheTimeout": 0,
      "url": "https://u.expo.dev/f2adb262-e091-47a0-bb3a-09b067fd148a"
    }
  }
}
